#: ../../index.md:96 8399749a1bcc44aca12b597d29c137a2
msgid ""
"Create AI agents with unique personalities that collaborate in thematic "
"circles:"
msgstr ""

#: ../../index.md:98 4732f8cdf56b49f3b387519825a412c2
msgid ""
"from gathering import Agent, Circle\n"
"\n"
"# Create agents with different providers\n"
"sophie = Agent(\n"
"    name=\"Sophie\",\n"
"    role=\"architect\",\n"
"    provider=\"anthropic\",\n"
"    model=\"claude-sonnet-4-20250514\"\n"
")\n"
"olivia = Agent(\n"
"    name=\"Olivia\",\n"
"    role=\"engineer\",\n"
"    provider=\"openai\",\n"
"    model=\"gpt-4o\"\n"
")\n"
"\n"
"# Create a circle\n"
"circle = Circle(name=\"Development Team\")\n"
"circle.add_agents([sophie, olivia])\n"
"\n"
"# Start a conversation\n"
"await circle.discuss(\"Design the new authentication system\")\n"
msgstr ""

