#: ../../user/installation.md:17 8172eaafefcf449f978a16208b20c147
msgid "1. Clone the Repository"
msgstr ""

#: ../../user/installation.md:19 8ef7adb9439f4d77af9b5ae5fe2b2f87
msgid ""
"git clone https://github.com/alkimya/gathering.git\n"
"cd gathering\n"
msgstr ""

#: ../../user/installation.md:24 4fed57b973cb43d98c37fc6301eb020c
msgid "2. Create Virtual Environment"
msgstr ""

#: ../../user/installation.md:26 69feab7f63584dbd964ea8823e8f58c3
msgid ""
"python -m venv venv\n"
"source venv/bin/activate  # Linux/macOS\n"
"# or\n"
"venv\\Scripts\\activate  # Windows\n"
msgstr ""

#: ../../user/installation.md:33 a04e52a135474a32b813aded32f37cb5
msgid "3. Install Python Dependencies"
msgstr ""

#: ../../user/installation.md:35 4aabd7f3ec06462d81a398e7be389b2b
msgid "pip install -r requirements.txt\n"
msgstr ""

#: ../../user/installation.md:39 611987f928894fef8579041efe50b7be
msgid "For development:"
msgstr ""

#: ../../user/installation.md:41 99ec6e2cbae44f50b4edf68ea99750f5
msgid ""
"pip install -r requirements-dev.txt\n"
"pip install -e .\n"
msgstr ""

#: ../../user/installation.md:46 2185e6a3d0c44bb599444ae8cfb1d8a8
msgid "4. Install Dashboard Dependencies"
msgstr ""

#: ../../user/installation.md:48 1046851dc374461caab903ff5efc8e16
msgid ""
"cd dashboard\n"
"npm install\n"
"cd ..\n"
msgstr ""

#: ../../user/installation.md:54 334d87fea1604deaa808235f3c941b75
msgid "5. Configure Environment"
msgstr ""

#: ../../user/installation.md:56 fe1af4bd09874b2981783d3bf170ade7
msgid "Copy the example environment file:"
msgstr ""

#: ../../user/installation.md:58 54a200dd6399496295e3c8da4df8674c
msgid "cp .env.example .env\n"
msgstr ""

#: ../../user/installation.md:62 ff48a3ac68ea499db84643ac390f71d3
msgid "Edit `.env` with your settings:"
msgstr ""

#: ../../user/installation.md:64 d0ec4595f6724929bffbe0932d419870
msgid ""
"# Database\n"
"DATABASE_URL=postgresql://user:password@localhost:5432/gathering\n"
"\n"
"# API Keys\n"
"OPENAI_API_KEY=sk-...\n"
"ANTHROPIC_API_KEY=sk-ant-...\n"
"\n"
"# Redis (optional)\n"
"REDIS_URL=redis://localhost:6379\n"
"\n"
"# Server\n"
"API_HOST=0.0.0.0\n"
"API_PORT=8000\n"
"\n"
"# Authentication (required for production)\n"
"SECRET_KEY=your-secret-key-min-32-characters\n"
"ADMIN_EMAIL=admin@example.com\n"
"ADMIN_PASSWORD_HASH=$2b$12$...  # Generate with: python -c \"from "
"passlib.context import CryptContext; "
"print(CryptContext(schemes=['bcrypt']).hash('your-password'))\"\n"
msgstr ""

#: ../../user/installation.md:85 6a41e31bcb594fe8913a7e369bdcfcf9
msgid "6. Setup Database"
msgstr ""

#: ../../user/installation.md:87 e7bedad7596944e48ab02b0f627ed960
msgid "Create the database and run migrations:"
msgstr ""

#: ../../user/installation.md:89 5549a117b6e74038a47f0d7a7268f0f2
msgid ""
"# Create database\n"
"createdb gathering\n"
"\n"
"# Enable pgvector extension\n"
"psql -d gathering -c \"CREATE EXTENSION IF NOT EXISTS vector;\"\n"
"\n"
"# Run migrations\n"
"python -m gathering.db.migrate\n"
msgstr ""

#: ../../user/installation.md:100 e059ad251d4c4950ba8f91e832e65997
msgid "7. Start the Application"
msgstr ""

#: ../../user/installation.md:102 2e11d125942748c5a9ac342d87415db3
msgid "Using the start script:"
msgstr ""

#: ../../user/installation.md:104 22478b469b6c4eb7a96a1f7fbcfc83e4
msgid "./scripts/start-workspace.sh\n"
msgstr ""

#: ../../user/installation.md:108 231c7d3df4f041b7905141b0b0261b6f
msgid "Or manually:"
msgstr ""

#: ../../user/installation.md:110 9d14c444ac1f4336a22dd64aeb0bb545
msgid ""
"# Terminal 1: Start API\n"
"uvicorn gathering.api:app --reload --port 8000\n"
"\n"
"# Terminal 2: Start Dashboard\n"
"cd dashboard && npm run dev\n"
msgstr ""

