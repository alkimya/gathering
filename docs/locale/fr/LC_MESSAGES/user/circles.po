#, python-brace-format
msgid ""
"from gathering import Circle\n"
"\n"
"circle = Circle(\n"
"    name=\"dev-team\",\n"
"    display_name=\"Development Team\",\n"
"    description=\"Frontend and backend developers\",\n"
"    config={\n"
"        \"max_agents\": 10,\n"
"        \"conversation_mode\": \"round_robin\"\n"
"    }\n"
")\n"
"await circle.save()\n"
msgstr ""

