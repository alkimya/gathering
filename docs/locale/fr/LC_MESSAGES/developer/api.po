"# gathering/api/dependencies.py\n"
"from fastapi import Depends, HTTPException\n"
"\n"
"async def get_agent_or_404(\n"
"    agent_id: int,\n"
"    db=Depends(get_db),\n"
"):\n"
"    agent = await db.fetch_one(\n"
"        \"SELECT * FROM agent.agents WHERE id = $1\",\n"
