#: ../../developer/testing.md:431 9e7fcc57888f444ba2bcb227f371cdd9
msgid "Best Practices"
msgstr ""

#: ../../developer/testing.md:433 769c6ae323f846709f3fdd8a5df4a293
msgid "1. Follow AAA Pattern"
msgstr ""

#: ../../developer/testing.md:435 d04277b1ec0a4c91a6396206e8fd3b7a
msgid ""
"def test_something():\n"
"    # Arrange - Set up test data\n"
"    data = create_test_data()\n"
"\n"
"    # Act - Perform the action\n"
"    result = do_something(data)\n"
"\n"
"    # Assert - Verify the result\n"
"    assert result == expected\n"
msgstr ""

#: ../../developer/testing.md:447 f9669edd7b2a466aa94bb54945e7b61d
msgid "2. One Assertion Per Test (When Possible)"
msgstr ""

#: ../../developer/testing.md:449 0318a84c99c448ae9646af5e321b610d
msgid ""
"# Good - focused tests\n"
"def test_agent_has_name():\n"
"    agent = Agent(name=\"Test\")\n"
"    assert agent.name == \"Test\"\n"
"\n"
"\n"
"def test_agent_has_default_provider():\n"
"    agent = Agent(name=\"Test\")\n"
"    assert agent.provider == \"anthropic\"  # default provider\n"
msgstr ""

#: ../../developer/testing.md:461 021237b727bb4e61a6d54b1a2691fb31
msgid "3. Use Descriptive Names"
msgstr ""

#: ../../developer/testing.md:463 9a97b42ecd0744cda4c299dd88db5c21
msgid ""
"# Good\n"
"def test_agent_raises_error_when_name_is_empty():\n"
"    with pytest.raises(ValueError):\n"
"        Agent(name=\"\")\n"
"\n"
"\n"
"# Bad\n"
"def test_agent_error():\n"
"    # ...\n"
msgstr ""

