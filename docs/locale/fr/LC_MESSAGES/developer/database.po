#: ../../developer/database.md:202 3f768aeaf5c142e685fb402a83158963
msgid "Similarity Search"
msgstr ""

#: ../../developer/database.md:204 62d63b54816c456490eeb7f043ab641b
msgid ""
"-- Find similar memories\n"
"SELECT id, key, value,\n"
"       1 - (embedding <=> query_embedding) AS similarity\n"
"FROM memory.memories\n"
"WHERE embedding IS NOT NULL\n"
"  AND agent_id = $1\n"
"ORDER BY embedding <=> query_embedding\n"
"LIMIT 10;\n"
msgstr ""

