#: ../../developer/database.md:7 9e9e2400758a460083a59438d586b372
msgid ""
"GatheRing uses [pycopg](../../pycopg/README.md), a high-level Python API "
"for PostgreSQL/PostGIS/TimescaleDB. It wraps `asyncpg` (async) and "
"`psycopg2` (sync) with a unified, pythonic interface."
msgstr ""

#: ../../developer/database.md:9 0e6974b0ef024b5ca2c32c2ddef80310
#, python-format
msgid ""
"from pycopg import Database, AsyncDatabase\n"
"\n"
"# Sync usage\n"
"db = Database.from_env()\n"
"db.list_schemas()\n"
"db.list_tables(\"agent\")\n"
"\n"
"# Async usage (recommended for API)\n"
"db = AsyncDatabase.from_env()\n"
"schemas = await db.list_schemas()\n"
"users = await db.execute(\"SELECT * FROM agent.agents WHERE status = "
"%s\", [\"active\"])\n"
"\n"
"# Connection pooling\n"
"from pycopg import PooledDatabase\n"
"\n"
"db = PooledDatabase.from_env(min_size=5, max_size=20)\n"
"with db.connection() as conn:\n"
"    result = conn.execute(\"SELECT * FROM agent.agents\")\n"
"\n"
"# Migrations\n"
"from pycopg import Migrator\n"
"\n"
"migrator = Migrator(db, \"gathering/db/migrations/\")\n"
"migrator.migrate()\n"
msgstr ""

